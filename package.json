{
  "name": "7.4.",
  "version": "1.0.0",
  "description": "Zadanie: Praca na branchach z konfliktami",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:html",
    "test:html": "nu-html-checker *.html",
    "build": "npm run build:sass && npm run build:autoprefixer && npm run test",
    "build:sass": "sass --sourcemap=none --update sass:css",
    "build:autoprefixer": "autoprefixer-cli css/style.css",
    "watch:sass": "sass --watch sass:css",
    "watch:autoprefixer": "onchange -d 100 css/style.css -- autoprefixer-cli -o css/style.prefixed.css css/style.css"
    "watch:browsersync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mkudlak/7.4..git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mkudlak/7.4./issues"
  },
  "homepage": "https://github.com/mkudlak/7.4.#readme",
  "dependencies": {
    "browser-sync": "^2.18.13",
    "glob-run": "^0.1.6",
    "node-sass": "^3.13.1",
    "onchange": "^3.2.1",
    "package.json": "^2.0.1"
  },
  "devDependencies": {}
}
